    "fastify": "^4.13.0"
  },
  "devDependencies": {
    "@types/fastify-multipart": "^0.7.0",
    "@types/node": "latest",
    "cross-env": "^7.0.3",
    "fastify-multipart": "^5.4.0",
    "fastify-tsconfig": "^1.0.1",
    "nodemon": "^2.0.21",
    "prettier": "^2.8.4",
